  "scripts": {
    "ng": "ng",
    "refresh-a": "ionic cordova platform rm android && ionic cordova platform add android",
    "bc": "ionic cordova build android",
    "bb": "ionic cordova build browser",
    "browser": "ionic cordova build browser -l",
    "resources": "cordova-res ios && cordova-res android && node scripts/resources.js",
    "prod": "ionic build --prod",
    "aab-all": "npm run bp && npm run aab && npm run copy2 && cd ../Prod/ && make sign2",
    "aab-release": "npm run bp && npm run aab && npm run copy2 && cd ../Prod/ && make sign2",
    "bp": "ionic cordova build android --prod --release",
    "aac": "cd platforms/android && gradlew bundle",
    "aab": "cd platforms/android && gradlew.bat bundle",
    "android": "ionic cordova emulate android --serverlogs --consolelogs --livereload --ssl",
    "copy2": "cp platforms/android/app/build/outputs/bundle/release/app-release.aab ../Prod",
    "ios": "ionic cordova emulate ios --serverlogs --consolelogs --livereload",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
